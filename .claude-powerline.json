{
  "theme": "dark",
  "display": {
    "style": "powerline",
    "lines": [
      {
        "segments": {
          "directory": { "enabled": true },
          "git": { "enabled": true },
          "model": { "enabled": true }
        }
      },
      {
        "segments": {
          "session": {
            "enabled": true,
            "type": "tokens",
            "showCompactCount": true
          }
        }
      },
      {
        "segments": {
          "context": {
            "enabled": true,
            "style": "bar",
            "barWidth": 10,
            "showBarPercentage": true,
            "showBarTokens": true,
            "thresholds": { "low": 50, "high": 80 }
          }
        }
      }
    ]
  }
}
